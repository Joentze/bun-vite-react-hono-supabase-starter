{
  "name": "bun-vite-react-hono-supabase-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "bun run dev:server & bun run dev:client & bun run db:start",
    "dev:client": "bunx vite",
    "dev:server": "bun run server/index.ts",
    "db:start": "supabase start",
    "db:stop": "supabase stop",
    "db:restart": "supabase stop && supabase start",
    "db:gen": "bunx drizzle-kit generate",
    "db:migrate": "bunx drizzle-kit migrate && bun run db:typegen",
    "db:typegen": "supabase gen types typescript --local > ./src/database.gen.ts",
    "build": "tsc -b && vite build",
    "test:server": "vitest",
    "test:server:coverage": "vitest run --coverage",
    "lint": "eslint .",
    "preview": "vite preview"
